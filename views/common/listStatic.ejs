<div class="fly-panel">
    <h3 class="fly-panel-title">温馨通道</h3>
    <ul class="fly-panel-main fly-list-static">
        <% if(selects) selects.forEach(function (item, index) { %>
        <li>
            <a href="/server/topics/topic/<%= item.tid %>" target="_blank"><%= item.title %></a>
        </li>
        <% }) %>
        <% if(selects.length==0) { %>
            <div class="fly-none" style="min-height:20px;">没有相关数据</div>
        <% } %>
    </ul>
</div>